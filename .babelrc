{
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage",
            "corejs": 3,
            "targets": { "node": "current" }
          }
        ],
        [
          "@babel/preset-react",
          { "runtime": "automatic", "importSource": "@emotion/react" }
        ],
        "@babel/preset-typescript"
      ],
      "plugins": ["@babel/plugin-transform-runtime", "@emotion"]
    },
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage",
            "corejs": 3,
            "targets": { "node": "current" }
          }
        ],
        [
          "@babel/preset-react",
          { "runtime": "automatic", "importSource": "@emotion/react" }
        ],
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/plugin-transform-runtime",
        "@emotion",
        "react-refresh/babel"
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltIns": "usage",
            "corejs": 3,
            "targets": { "node": "current" }
          }
        ],
        [
          "@babel/preset-react",
          { "runtime": "automatic", "importSource": "@emotion/react" }
        ],
        "@babel/preset-typescript"
      ],
      "plugins": ["@babel/plugin-transform-runtime", "@emotion"]
    }
  }
}
