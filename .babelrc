{
  "presets": [
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      { "useBuiltIns": "usage", "corejs": 3, "targets": { "node": "current" } }
    ],
    [
      "@babel/preset-react",
      { "runtime": "automatic", "importSource": "@emotion/react" }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@emotion",
    "react-refresh/babel"
  ]
}
